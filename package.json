{
  "name": "cubing",
  "version": "0.10.0",
  "description": "A collection of Javascript cubing libraries.",
  "author": "The js.cubing.net team",
  "main": "dist/cjs/cubing.js",
  "types": "dist/cjs/cubing/index.d.ts",
  "license": "GPL-3.0-or-later",
  "repository": "github:cubing/cubing.js",
  "dependencies": {
    "@types/resize-observer-browser": "^0.1.2",
    "@types/three": "^0.103.2",
    "@types/web-bluetooth": "0.0.4",
    "babel-polyfill": "^6.26.0",
    "three": "^0.111.0",
    "three-orbit-controls": "^82.1.0",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.2",
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-jest": "^24.8.0",
    "eslint": "^6.7.2",
    "jest": "^24.8.0",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-pegjs": "0.0.2",
    "pegjs": "^0.11.0-master.b7b87ea",
    "pegjs-jest": "0.0.2",
    "rollup": "^1.16.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-notify": "^1.1.0",
    "rollup-plugin-pegjs": "^2.1.3",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript2": "^0.21.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.5.2",
    "typescript": "^3.5.2"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -cw",
    "clean": "rm -rf dist .cache .rpt2_cache",
    "test": "jest && npm run lint",
    "format": "tslint --fix --project .",
    "setup": "npm install",
    "lint": "tslint --fix --project . ; tslint --fix rollup.config.js",
    "prepack": "npm run clean && npm run build",
    "parcel": "parcel serve app/index.html --open",
    "parcel-build-for-vr-cubing-net": "parcel build --public-url ./ --out-dir dist/vr app/vr/index.html",
    "parcel-build-for-experiments-cubing-net": "parcel build --public-url /cubing.js/ app/index.html"
  },
  "bin": {
    "puzzle-geometry": "./dist/bin/puzzle-geometry-bin.js"
  },
  "keywords": [
    "cubing",
    "speedcubing",
    "puzzle",
    "visualization",
    "kpuzzle",
    "cuble",
    "bluetooth",
    "rubik",
    "rubik's"
  ]
}
